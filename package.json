{
  "name": "ali-oss-upload",
  "version": "1.0.0",
  "description": "support uploading files to ali oss",
  "main": "index.js",
  "types": "src/index.d.ts",
  "files": [
    "src",
    "lib"
  ],
  "scripts": {
    "build:type": "tsc",
    "eslint": "eslint . --ext .js,.ts --fix",
    "prettier": "prettier --write \"./**/*.{ts,js,json}\"",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/weirui88888/ali-oss-upload.git"
  },
  "keywords": [
    "alioss",
    "upload"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/weirui88888/ali-oss-upload/issues"
  },
  "homepage": "https://github.com/weirui88888/ali-oss-upload#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "mrm": "^4.1.13",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "npx --no-install commitlint --edit $1"
    }
  },
  "lint-staged": {
    "*.ts": "npm run eslint",
    "*.ts": "npm run prettier"
  },
  "dependencies": {
    "@types/ali-oss": "^6.16.7",
    "ali-oss": "^6.17.1"
  }
}
